<resources>
	<declare-styleable name="TreeLayout">
		<attr name="direction" format="enum">
			<enum name="top_To_Bottom" value="1" />
			<enum name="bottom_To_Top" value="2" />
			<enum name="left_To_Right" value="3" />
			<enum name="right_To_Left" value="4" />
		</attr>
		<attr name="equalChildSize" format="boolean" />
	</declare-styleable>
	<declare-styleable name="TreeLayout_Layout">
		<attr name="mother" format="reference" />
		<attr name="father" format="reference" />
		<attr name="level" format="integer" />
		<attr name="children" format="integer"/>
	</declare-styleable>
</resources>