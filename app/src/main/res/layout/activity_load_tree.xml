<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	tools:context=".controllers.LoadTreeActivity">
	
	<com.google.android.material.floatingactionbutton.FloatingActionButton
		android:id="@+id/fabBackToLaunch"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginStart="5dp"
		android:layout_marginTop="5dp"
		android:clickable="true"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent"
		app:srcCompat="@drawable/previous_back" />
	
	<androidx.recyclerview.widget.RecyclerView
		android:id="@+id/recyclerTree"
		android:layout_width="0dp"
		android:layout_height="0dp"
		android:padding="45dp"
		app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/txtSlctTree">
	
	</androidx.recyclerview.widget.RecyclerView>
	
	<TextView
		android:id="@+id/txtSlctTree"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Select a family tree"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>